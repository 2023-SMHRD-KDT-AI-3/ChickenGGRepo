  	<display-name>CheckBrand</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.controller.CheckBrand</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GoMain</servlet-name>
  	<url-pattern>/goMain</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginService</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterService</servlet-name>
  	<url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CheckUser</servlet-name>
  	<url-pattern>/CheckUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CheckBrand</servlet-name>
  	<url-pattern>/CheckBrand</url-pattern>
  </servlet-mapping>
</web-app>
