<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GoMain</servlet-name>
  	<display-name>GoMain</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.controller.GoMain</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginService</servlet-name>
  	<display-name>LoginService</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.controller.LoginService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterService</servlet-name>
  	<display-name>RegisterService</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.controller.RegisterService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CheckUser</servlet-name>
  	<display-name>CheckUser</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.controller.CheckUser</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Logout</servlet-name>
  	<display-name>Logout</display-name>
  	<description></description>
  	<servlet-class>com.smhrd.controller.Logout</servlet-class>
  </servlet>
  <servlet>
